<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@mipmap/img_login"
    tools:context=".views.WorldkieMenu">

    <ImageView
        android:id="@+id/iv_scenariokies_worldkie_menu"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginTop="32dp"
        android:scaleType="centerCrop"
        android:src="@mipmap/img_scenariokies_worldkie_menu"
        app:layout_constraintEnd_toEndOf="@+id/textView5"
        app:layout_constraintStart_toStartOf="@+id/textView5"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <ImageView
        android:id="@+id/iv_stuffkies_worldkie_menu"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginTop="32dp"
        android:scaleType="centerCrop"
        android:src="@mipmap/img_stuffkies_worldkie_menu"
        app:layout_constraintEnd_toEndOf="@+id/textView8"
        app:layout_constraintStart_toStartOf="@+id/textView8"
        app:layout_constraintTop_toBottomOf="@+id/tv_characterkies" />

    <Button
        android:id="@+id/textView5"
        style="@style/Widget.Material3.Button.OutlinedButton"
        android:layout_width="200dp"
        android:layout_height="51dp"
        android:backgroundTint="#B39DDB"
        android:gravity="center"
        android:text="Scenariokies"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        android:textStyle="bold"
        app:icon="@drawable/twotone_map_24"
        app:iconGravity="start"
        app:iconTint="#FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.551"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iv_scenariokies_worldkie_menu"
        app:strokeColor="#FFFFFF"
        app:strokeWidth="3dp" />

    <Button
        android:id="@+id/textView8"
        style="@style/Widget.Material3.Button.OutlinedButton"
        android:layout_width="200dp"
        android:layout_height="51dp"
        android:backgroundTint="#B39DDB"
        android:gravity="center"
        android:text="Stuffikies"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        android:textStyle="bold"
        app:icon="@drawable/twotone_catching_pokemon_24"
        app:iconGravity="start"
        app:iconTint="#FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iv_stuffkies_worldkie_menu"
        app:strokeColor="#FFFFFF"
        app:strokeWidth="3dp" />

    <Button
        android:id="@+id/tv_characterkies"
        style="@style/Widget.Material3.Button.OutlinedButton"
        android:layout_width="200dp"
        android:layout_height="51dp"
        android:backgroundTint="#B39DDB"
        android:gravity="center"
        android:text="Characterkies"
        android:textColor="@color/white"
        android:textSize="24sp"
        android:textStyle="bold"
        app:icon="@drawable/twotone_face_retouching_natural_24"
        app:iconGravity="start"
        app:iconTint="#FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iv_characterkies_worldkie_menu"
        app:strokeColor="#FFFFFF"
        app:strokeWidth="3dp" />

    <ImageView
        android:id="@+id/iv_characterkies_worldkie_menu"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginTop="178dp"
        android:scaleType="centerCrop"
        android:src="@mipmap/img_characterkies_worldkie_menu"
        app:layout_constraintEnd_toEndOf="@+id/tv_characterkies"
        app:layout_constraintStart_toStartOf="@+id/tv_characterkies"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/button_backgground"
        tools:layout_editor_absoluteX="268dp"
        tools:layout_editor_absoluteY="228dp" />

</androidx.constraintlayout.widget.ConstraintLayout>