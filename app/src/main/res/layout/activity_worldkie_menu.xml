<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@mipmap/img_login"
    tools:context=".WorldkieMenu">

    <TextView
        android:id="@+id/textView10"
        android:layout_width="123dp"
        android:layout_height="50dp"
        android:layout_marginStart="96dp"
        android:layout_marginTop="105dp"
        android:background="@drawable/button_backgground"
        android:gravity="center"
        android:text="Waykies"
        android:textColor="#FFFFFF"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_characterkies" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="123dp"
        android:layout_height="50dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="34dp"
        android:background="@drawable/button_backgground"
        android:foreground="@drawable/twotone_map_24"
        android:foregroundGravity="left|center"
        android:foregroundTint="#FFFFFF"
        android:gravity="center"
        android:text="Regionkies"
        android:textColor="#FFFFFF"
        app:layout_constraintStart_toEndOf="@+id/textView10"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="123dp"
        android:layout_height="50dp"
        android:layout_marginTop="21dp"
        android:layout_marginEnd="49dp"
        android:background="@drawable/button_backgground"
        android:gravity="center"
        android:text="Stuffikies"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_characterkies" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="77dp"
        android:layout_marginEnd="74dp"
        android:scaleType="fitCenter"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/twotone_catching_pokemon_24" />

    <TextView
        android:id="@+id/tv_characterkies"
        android:layout_width="140dp"
        android:layout_height="51dp"
        android:layout_marginTop="116dp"
        android:background="@drawable/button_backgground"
        android:foreground="@drawable/twotone_face_retouching_natural_24"
        android:foregroundGravity="left|center"
        android:foregroundTint="#FFFFFF"
        android:gravity="center"
        android:text="Characterkies"
        android:textColor="@color/white"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="106dp" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="18dp"
        android:layout_marginTop="101dp"
        android:scaleType="fitCenter"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/twotone_face_retouching_natural_24" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="63dp"
        android:scaleType="fitCenter"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView"
        app:srcCompat="@drawable/twotone_map_24" />

</androidx.constraintlayout.widget.ConstraintLayout>