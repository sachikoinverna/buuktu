<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@null">

    <com.google.android.material.card.MaterialCardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        card_view:cardCornerRadius="10dp"
        card_view:cardElevation="10dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="horizontal">

                <ImageButton
                    android:id="@+id/ib_prf_imgOne"
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:background="@null"
                    tools:srcCompat="@mipmap/photoprofileone" />

                <ImageButton
                    android:id="@+id/ib_prf_imgTwo"
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:background="@null"
                    tools:srcCompat="@mipmap/photoprofiletwo" />

                <ImageButton
                    android:id="@+id/ib_prf_imgThree"
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:background="@null"
                    tools:srcCompat="@mipmap/photoprofilethree" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="horizontal">

                <ImageButton
                    android:id="@+id/ib_prf_imgFour"
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:background="@null"

                    tools:srcCompat="@mipmap/photoprofilefour" />

                <ImageButton
                    android:id="@+id/ib_prf_imgFive"
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:background="@null"

                    tools:srcCompat="@mipmap/photoprofilefive" />

                <ImageButton
                    android:id="@+id/ib_prf_imgSix"
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:background="@null"

                    tools:srcCompat="@mipmap/photoprofilesix" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="horizontal">

                <ImageButton
                    android:id="@+id/ib_prf_imgSeven"
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:background="@null"

                    tools:srcCompat="@mipmap/photoprofileseven" />

                <ImageButton
                    android:id="@+id/ib_prf_imgEight"
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:background="@null"

                    tools:srcCompat="@mipmap/photoprofileeight" />

                <ImageButton
                    android:id="@+id/ib_prf_imgNine"
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:background="@null"

                    tools:srcCompat="@mipmap/photoprofilenine" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="horizontal">

                <ImageButton
                    android:id="@+id/ib_prf_imgTen"
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:background="@null"

                    tools:srcCompat="@mipmap/photoprofileten" />

                <ImageButton
                    android:id="@+id/ib_prf_imgEleven"
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:background="@null"

                    tools:srcCompat="@mipmap/photoprofileeleven" />
            </LinearLayout>

        </LinearLayout>

    </com.google.android.material.card.MaterialCardView>
</androidx.constraintlayout.widget.ConstraintLayout>